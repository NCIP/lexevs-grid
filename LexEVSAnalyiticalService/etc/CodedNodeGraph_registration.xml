<?xml version="1.0" encoding="UTF-8" ?>
<ServiceGroupRegistrationParameters xmlns:sgc="http://mds.globus.org/servicegroup/client"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:wsa="http://schemas.xmlsoap.org/ws/2004/03/addressing"
  xmlns:agg="http://mds.globus.org/aggregator/types"
  xmlns="http://mds.globus.org/servicegroup/client">
  <!-- The ServiceGroupEPR defines the servicegroup to which registrations will be made -->
  <ServiceGroupEPR>
    <wsa:Address>INDEX_SERVICE_URL</wsa:Address>
  </ServiceGroupEPR>
  <!-- Specifies that the registration will be renewed every 10 minutes -->
  <RefreshIntervalSecs>REGISTRATION_REFRESH</RefreshIntervalSecs>
  <Content xsi:type="agg:AggregatorContent" xmlns:agg="http://mds.globus.org/aggregator/types">
    <agg:AggregatorConfig xsi:type="agg:AggregatorConfig">
      <agg:GetMultipleResourcePropertiesPollType
			xmlns:ns13="gme://caCORE.caCORE/3.2/gov.nih.nci.evs.security"
			xmlns:ns26="http://LexBIGCaGridServices.cagrid.LexBIG.LexGrid.org/LexBIGCaGridServices/Filter/types"
			xmlns:ns20="http://LexBIGCaGridServices.cagrid.LexBIG.LexGrid.org/LexBIGCaGridServices/CodedNodeGraph/types"
			xmlns:ns23="http://LexBIGCaGridServices.cagrid.LexBIG.LexGrid.org/LexBIGCaGridServices/HistoryService/types"
			xmlns:ns14="http://schemas.xmlsoap.org/ws/2004/03/addressing"
			xmlns:ns0="uri:iso.org:21090"
			xmlns:ns1="http://LexGrid.org/schema/2010/01/LexGrid/iso21090/codingSchemes"
			xmlns:ns11="http://LexGrid.org/schema/LexBIG/iso21090/2010/01/enums"
			xmlns:ns3="http://LexGrid.org/schema/LexBIG/iso21090/2010/01/Core"
			xmlns:ns2="http://LexGrid.org/schema/LexBIG/iso21090/2010/01/Collections"
			xmlns:ns7="http://LexGrid.org/schema/2010/01/LexGrid/iso21090/concepts"
			xmlns:ns6="http://LexGrid.org/schema/2010/01/LexGrid/iso21090/NCIHistory"
			xmlns:ns22="http://LexBIGCaGridServices.cagrid.LexBIG.LexGrid.org/LexBIGCaGridServices/ResolvedConceptReferencesIterator/types"
			xmlns:ns9="http://LexGrid.org/schema/LexBIG/iso21090/2010/01/InterfaceElements"
			xmlns:ns4="http://LexGrid.org/schema/2010/01/LexGrid/iso21090/commonTypes"
			xmlns:ns8="http://LexGrid.org/schema/2010/01/LexGrid/iso21090/versions"
			xmlns:ns15="http://LexBIGCaGridServices.cagrid.LexBIG.LexGrid.org/LexBIGCaGridServices/types"
			xmlns:ns16="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"
			xmlns:ns5="http://LexGrid.org/schema/2010/01/LexGrid/iso21090/relations"
			xmlns:ns10="http://LexGrid.org/schema/LexBIG/iso21090/2010/01/caGrid"
			xmlns:ns17="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata"
			xmlns:ns25="http://LexBIGCaGridServices.cagrid.LexBIG.LexGrid.org/LexBIGCaGridServices/Sort/types"
			xmlns:ns18="http://LexBIGCaGridServices.cagrid.LexBIG.LexGrid.org/LexBIGCaGridServices/CodedNodeSet/types"
			xmlns:ns21="http://LexBIGCaGridServices.cagrid.LexBIG.LexGrid.org/LexBIGCaGridServices/LexBIGServiceConvenienceMethods/types"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:ns24="http://LexBIGCaGridServices.cagrid.LexBIG.LexGrid.org/LexBIGCaGridServices/LexBIGServiceMetadata/types"
			xmlns:ns19="http://docs.oasis-open.org/wsrf/2004/06/wsrf-WS-ResourceLifetime-1.2-draft-01.xsd"
			xmlns:ns12="http://LexGrid.org/schema/2010/01/LexGrid/iso21090/NCIChangeTypes">
        <!-- Specifies that the index should refresh information
            every 300000 milliseconds (once every 5 minutes) -->
        <agg:PollIntervalMillis>INDEX_REFRESH</agg:PollIntervalMillis>
        <!-- specifies all Resource Properties that should be retrieved from the service -->
      </agg:GetMultipleResourcePropertiesPollType>
    </agg:AggregatorConfig>
    <agg:AggregatorData/>
  </Content>
</ServiceGroupRegistrationParameters>
